{
  "name": "book-app-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare-husky": "husky install && npx husky add .husky/pre-commit 'yarn precommit'",
    "dev": "vite --open",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "precommit": "yarn build && lint-staged",
    "lint-ts": "eslint . --ext .ts,.tsx",
    "lint-ts-fix": "eslint . --ext .ts,.tsx --fix",
    "lint-styles": "stylelint \"**/*.{css,scss,sass}\"",
    "lint-styles-fix": "stylelint \"**/*.{css,scss,sass}\" --fix"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "classnames": "^2.3.2",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.6.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "5.47.0",
    "@typescript-eslint/parser": "5.47.0",
    "@vitejs/plugin-react-swc": "^3.0.1",
    "eslint": "8.30.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react": "7.31.11",
    "husky": "8.0.2",
    "lint-staged": "13.1.0",
    "postcss-scss": "^4.0.6",
    "sass": "^1.57.1",
    "stylelint": "14.16.0",
    "stylelint-scss": "4.3.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vite-plugin-svgr": "2.4.0"
  }
}
